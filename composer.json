{
  "name": "manialab/maniaprintlab",
  "description": "Adds badge configuration, printing and history to Laravel Nova",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "ManiaLab\\ManiaPrintLab\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ManiaLab\\ManiaPrintLab\\ManiaPrintLabServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan vendor:publish --tag=maniaprintlab-migrations --ansi"
    ]
  }
}
